    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "komodo-chainparams",
            "args": ["-ac_name=MCL", "-ac_supply=2000000", "-ac_cc=2", "-addnode=5.189.149.242", "-addnode=161.97.146.150", "-addnode=149.202.158.145", "-addressindex=1", "-spentindex=1", "-ac_marmara=1", "-ac_staked=75", "-ac_reward=3000000000"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Project"
        }

    ]
}